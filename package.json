{
  "name": "zulipbot",
  "version": "1.0.0",
  "description": "GitHub workflow bot for @zulip",
  "main": "src/index.js",
  "engines": {
    "node": "*",
    "npm": "*"
  },
  "files": [
    "src"
  ],
  "dependencies": {
    "github": "^8.2.1",
    "express": "^4.14.0",
    "ejs": "^2.5.5",
    "body-parser": "^1.16.0",
    "request": "^2.79.0"
  },
  "devDependencies": {
    "eslint": "^3.16.1",
    "eslint-config-xo-space": "^0.15.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-exclude-gitignore": "^1.1.1",
    "gulp-line-ending-corrector": "^1.0.1",
    "gulp-istanbul": "^1.1.1",
    "gulp-mocha": "^4.0.1",
    "gulp-plumber": "^1.1.0",
    "gulp-nsp": "^2.4.2",
    "gulp-coveralls": "^0.1.4"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "node": true,
      "es6": true,
      "mocha": true
    },
    "rules": {
      "quotes": ["error", "double"],
      "indent": ["error", 2, { "ObjectExpression": 1 }],
      "space-before-function-paren": ["error", "never"],
      "curly": ["error", "multi-line"],
      "one-var": ["error", { "initialized": "never" }],
      "camelcase": "off",
      "arrow-parens": [2, "as-needed", { "requireForBlockBody": true }],
      "max-params": ["error", 5]
    }
  },
  "scripts": {
    "start": "node ./src/index.js",
    "prepublish": "gulp prepublish",
    "test": "gulp"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/zulip/zulipbot.git"
  },
  "keywords": [
    "zulip",
    "bot",
    "bots",
    "github",
    "workflow",
    "zulipbot",
    "synicalsyntax"
  ],
  "author": "Cynthia Lin <cynthialin1004@gmail.com> (http://clin-dump.co.nf)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/zulip/zulipbot/issues"
  },
  "homepage": "https://github.com/zulip/zulipbot#readme"
}
